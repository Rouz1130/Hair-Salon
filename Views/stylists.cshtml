<h1>Hair Salon Database</h1>
<br>
<h1>Stylists:</h1>
<ul>
  @if (Model.Count != 0)
  {
    @foreach (var stylist in Model)
    {
      <li><a href="/stylists/@stylist.GetId()">@stylist.GetName()</a> | <a href="/stylist/edit/@stylist.GetId()">Edit</a>| <a href="stylist/delete/@stylist.GetId()">Delete</a> </li>
    }
  }
</ul>
<h4><a href="/stylists/new">Add Stylist</a></h4>
<h4><a href="/">Home</a></h4>

<form action="/stylists/clear" method="post">
  <button type="submit">Delete</button>
</form>
